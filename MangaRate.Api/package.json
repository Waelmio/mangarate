{
  "name": "mangarate",
  "version": "0.0.1",
  "description": "A project to always be up to date on your favorite manga, using your favorite fournisseur !",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev2": "ts-node-dev --respawn --pretty --transpile-only src/index.ts",
    "lint": "eslint . --ext .ts",
    "start": "node build/src/index.js",
    "build": "tsoa spec-and-routes && tsc",
    "dev": "concurrently \"nodemon\" \"nodemon -x tsoa spec-and-routes\"",
    "seed": "npm run build && node build/src/scripts/generator.js"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts",
    "exec": "ts-node src/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Waelmio/mangarate.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Waelmio/mangarate/issues"
  },
  "homepage": "https://github.com/Waelmio/mangarate#readme",
  "dependencies": {
    "@types/body-parser": "^1.19.2",
    "axios": "^0.26.0",
    "body-parser": "^1.19.2",
    "cheerio": "^1.0.0-rc.10",
    "config": "^3.3.7",
    "cors": "^2.8.5",
    "eslint-config-prettier": "^8.4.0",
    "express": "^4.17.2",
    "helmet": "^5.0.2",
    "morgan": "^1.10.0",
    "pg": "^8.7.3",
    "postgrator": "^5.0.0",
    "swagger-ui-express": "^4.3.0",
    "tldjs": "^2.3.1",
    "tslog": "^3.3.2",
    "tsoa": "^3.2.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^6.2.0",
    "@types/config": "0.0.41",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.18",
    "@types/pg": "^8.6.4",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/tldjs": "^2.3.1",
    "@types/xmldom": "^0.1.31",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "concurrently": "^7.0.0",
    "eslint": "^8.9.0",
    "nodemon": "^2.0.15",
    "ts-node": "^10.5.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.5.5"
  }
}
